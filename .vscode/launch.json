  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch via NPM",
      "request": "launch",
      "runtimeArgs": ["run", "start"],
      "runtimeExecutable": "npm",
      "skipFiles": ["<node_internals>/**"],
      "type": "node"
    },
    {
      "type": "pwa-chrome",
      "request": "launch",
      "name": "Launch Chrome 8001",
      "url": "http://localhost:8001",
      "webRoot": "${workspaceFolder}"
    }
  ]
}
